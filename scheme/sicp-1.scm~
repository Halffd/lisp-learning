(define size 2)
(* 4 size)
(define (square x) (* x x))
(define (sum-of-squares x y)
  (+ (square x) (square y)))

(Sum-of-squares 3 4)
(define (f a)
  (sum-of-squares (+ a 1) (* a 2)))

(f 5)

(define (abs x)
  (cond ((> x 0) x)
        ((= x 0) 0)
        ((< x 0) (- x))))
(define (abss x)
  (cond ((> x 7) x)
        ((= x 1) 0)
        ((< x 6) (- x))))

(abss 7)
(define (abs x)
  (cond ((< x 0) (- x))
        (else x)))
(abs -44)
(define (abs x)
  (if (< x 0)
      (- x)
      x))
(define x (square (abs -72)))
(and (> x 5) (< x 10))
(and (> x 5) (< x 1909999900))

(define x 4)
(define y 4)
(define (>= x y)
(or (> x y) (= x y)))
(define x 7)
(define (>= x y)
  (not (< x y)))

